~aaa
    role root shy
~ssh
    target web.code.project.project

~web.code.tmux
    make golang init "1 u -p 30" "2 v" \
        "1 k 'vim -O src/main.go src/main.shy' Enter" \
        "2 k 'tail -f bin/boot.log' Enter" \
        "3 k 'bin/ice.sh shutdown' Enter" \
        "3 k 'bin/ice.sh start serve random dev' Enter"
    make shy link miss
